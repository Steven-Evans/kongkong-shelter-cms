{
  "kind": "collectionType",
  "collectionName": "postings",
  "info": {
    "name": "posting"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "description": {
      "type": "richtext",
      "private": false,
      "required": true
    },
    "media": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
